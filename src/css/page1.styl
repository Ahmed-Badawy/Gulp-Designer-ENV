h1{
	background-color yellow
}


.btn
	font-family tahoma
	color gr
	&:hover
		color yellow
	&:active
		color gr
		font-family family_one
		


@keyframes example {
    from {background-color: red;}
    to {background-color: yellow;}
}

/* The element to apply the animation to */
div {
    width: 100px;
    height: 100px;
    background-color: red;
    animation-name: example;
    animation-duration: 4s;
}

